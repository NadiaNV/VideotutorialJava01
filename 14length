class length {
    public static void main (String[] args) {
        String respuesta = "amarillo";
        //respuenta.length(); n√∫mero de caracteres de la variable "largura"
        int tam = respuesta.length();
        System.out.println(tam);
        //amarillo.lenght(); devuelve 8
    }
}
